{% extends 'base.html.twig' %}


{% block stylesheets %}{{ parent() }}{% endblock %}
{% block title %}{{ parent()}} HomeController!{% endblock %}

{% block header %} {{ parent() }} {% endblock %}
{% block body %}
    {% for ValeurList in HomeList %}
        <li>{{ 'annoucements.title'|trans }} : {{ ValeurList.title }}</li><br>
        <li>{{ 'annoucements.content'|trans }} : {{ ValeurList.content }}</li><br>
        <li>{{ 'annoucements.price'|trans }} : {{ ValeurList.price }}</li><br>
        <li>{{ 'annoucements.date'|trans }} : {{ ValeurList.createdAt |date("d/m/Y") }}</li>
        <a href={{ path('detail',{id: ValeurList.id}) }}>{{ 'annoucements.link'|trans }}</a><br><br>
    {% endfor %}
{% endblock %}